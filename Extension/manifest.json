{
    "manifest_version": 2,
    "name": "Phisher Sniffer",
    "version": "1.0",
    "description": "Phisher Sniffer detects if a website is a phishing website of a legitimate website.",
    "icons": {
        "16":"icons/scam-alert16.png",
        "48":"icons/scam-alert48.png",
        "128":"icons/scam-alert128.png"
    },
    "browser_action": {
        "default_icon": "icons/scam-alert16.png",
        "default_popup": "html/popup.html"
    },
    "permissions": ["tabs", "activeTab", "http://*/*", "https://*/*"],
    "background": {
        "scripts": ["js/background.js"]
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["js/content.js"],
          "run_at": "document_end"
        }
    ],
    "author": "Reconnaissance Technologies",
    "offline_enabled": true,
    "version_name": "1.0.0-dev"
  }